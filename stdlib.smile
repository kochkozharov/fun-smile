((F_F)>> fold :@
    :F f (/>_<)/~o :F i (/>_<)/~o :F l (/>_<)/~o 
    (?_?) _ B= l []
    (T_T) i
    (E_E) _ f _ head l _ fold f i _ tail l

(F_F)> sub1 :@ 
    :F x (/>_<)/~o _ _ :- x 1

((F_F)>> fact :@
    :F x (/>_<)/~o 
        (?_?) _ <B= x 1
        (T_T) 1
        (E_E) _ _ :* x _ fact _ sub1 x
